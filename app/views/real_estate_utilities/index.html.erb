<p id="notice"><%= notice %></p>

<h1>Listing Real Estate Utilities</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th>Options</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @real_estate_utilities.each do |real_estate_utility| %>
      <tr>
        <td><%= real_estate_utility.name %></td>
        <td><%= real_estate_utility.code %></td>
        <td><%= real_estate_utility.options %></td>
        <td><%= link_to 'Show', real_estate_utility %></td>
        <td><%= link_to 'Edit', edit_real_estate_utility_path(real_estate_utility) %></td>
        <td><%= link_to 'Destroy', real_estate_utility, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Real estate utility', new_real_estate_utility_path %>
