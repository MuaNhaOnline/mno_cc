<%= request.user_agent %>

<% #rand = Random.new %>

<% (1...1).each do %>
	<% 
		referrer_host_name = case rand.rand(1..10)
		when 1,2,3
			'Facebook'
		when 4,5,6,7
			'Google'
		when 8,9
			'VnExpress'
		else
			'Youtube'
		end

		day_ago = rand.rand(0...1095)

		campaign = case rand.rand(1..20)
		when 1,2,3,4
			'Increase vistor campaign'
		when 5,6
			'PR villa products'
		when 7,8,9,10
			'Cheapest with Best'
		when 11,12,13,14,15
			'Every body has home'
		when 16,17,18
			'Your home'
		when 19
			'Easy easy and easy'
		else
			'Go with me'
		end

		type = case rand.rand(1..4)
		when 1
			"'contact'"
		else
			'nil'
		end
	%>
	
	{ referrer_host_name: '<%= referrer_host_name %>', created_at: <%= day_ago %>.day.ago, utm_campaign: '<%= campaign %>', user_info_type: <%= type %> },
<% end %>
