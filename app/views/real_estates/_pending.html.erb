<% res.each do |re| %>
	<article class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
		<article class="item" data-value="<%= re.id %>">
			<section class="top">
				<a href="/bat-dong-san/<%= re.full_slug %>">
					<%= real_estate_avatar re %>
				</a>
				<section class="left">
					<article class="node">
						<span><b><%= re.display_id %></b></span>
					</article>
				</section>
			</section>
			<section class="bottom">
				<p class="title">
					<%= re.title %>
				</p>
				<p>
					<span class="fa fa-fw fa-user"></span> <%= re.user_id == 0 ? re.contact_user.name : "<a href=\"/thanh-vien/#{re.user.id}\">#{re.user.full_name}</a>".html_safe %>
				</p>
				<p>
					<span class="fa fa-fw fa-home"></span> <%= re.display_real_estate_type %>
				</p>
				<p>
					<span class="fa fa-fw fa-tag"></span> <%= re.display_purpose %>
				</p>
				<p class="address">
					<span class="fa fa-fw fa-map-marker"></span> <%= re.display_address %>
				</p>
			</section>
			<section class="control">
				<span class="node">
					<a aria-click="approve" href="javascript:void(0)" class="fa fa-check"></a>
				</span>
				<span class="node">
					<a aria-click="delete" href="javascript:void(0)" class="fa fa-trash-o fa-lg"></a>
				</span>
			</section>
		</article>
	</article>
<% end %>