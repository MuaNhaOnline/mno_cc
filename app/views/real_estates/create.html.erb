<% content_for :stylesheets do %>
    <link rel="stylesheet" href="/assets/real_estates/create.css" />
<% end %>

<% content_for :scripts do %>
    <script src="/assets/real_estates/create.js"></script>
<% end %>

<div class="container-fluid">
  <!-- #start nav -->
  <nav class="row">    
    <ul>
      <li><a href="../../index.html"><img src="/assets/icon/home-26.png"></a></li>
      <li><a href="#">Đăng tin</a></li>      
    </ul>
  </nav>
  <!-- #end nav -->
  <!-- #start main-content -->
  <article class="row wrapper">
    <article class="main-content col-md-8">    
      <form class="form-horizontal" id="create_real_estate" role="form" method="POST" action="" accept-charset="UTF-8" enctype="application/x-www-form-urlencoded">
      <section class="main-title">Thông tin rao bán của bạn</section>      
      <ul class="item-wrapper">
        <!-- #Start item "Loại hình giao dịch" -->
        <li class="item">
          <div class="item-title">Loại hình giao dịch</div>
          <ul class="item-content">
            <li class="form-group">            
              <div class="col-sm-4 col-sm-offset-3">
                <select name="purpose" class="form-control will-change-select" id="purpose">
                  <option data-show="sell" data-hide="un-sell">Bán</option>
                  <option data-show="rent" data-hide="un-rent">Cho thuê</option>
                </select>
              </div>
            </li>      
          </ul>
        </li>
        <!-- #End item "Loại hình giao dịch" -->
        <!-- #Start item "Giá bán mong muốn" -->
        <li class="item">
          <div class="item-title">
            Giá bán mong muốn
            <a href="#" class="support-button" data-value="#support_price"></a>
            <section class="support-text" id="support_price">
              <b>Giá bán mong muốn:</b> Quý khách có thể sử dụng chức năng <a href="#">"định giá nhà"</a> hoặc <a href="#">"định giá khu vực"</a> để tham khảo giá thị trường nhà đất giao dịch gần nhất.<br>
              <b>Thỏa thuận:</b> Giá bán sẽ được thỏa thuận khi Người mua gặp trực tiếp Người bán.
            </section>
          </div>
          <ul class="item-content">
            <li class="form-group">            
              <label for="price" class="control-label col-sm-3">Giá mong muốn:</label>                                
              <div class="col-sm-4">
                <input name="price" class="form-control required only-number" id="price" data-separate='.' data-controlled="|price-agree|">
              </div>
              <div class="checkbox col-sm-4">
                <label><input type="checkbox" class="will-change-available-check" data-disable="price-agree"> Giá Thoả thuận</label>
              </div>                 
            </li>
            <li class="form-group">            
              <label for="currency_id" class="control-label col-sm-3">Đơn vị tính:</label>
              <div class="col-sm-3">
                <select name="currency_id" class="form-control" id="currency_id">
                  <option data-value="VND">VNĐ</option>
                  <option data-value="USD">USD</option>
                  <option data-value="SJC">SJC</option>
                </select>
              </div>
              <label for="unit" class="control-label col-sm-1">trên</label>
              <div class="col-sm-4"> 
                <select class="form-control" id="unit" data-controlled="|sell|un-rent|">  
                  <option>Tổng diện tích</option>
                  <option>Mét vuông</option>  
                </select>
                <select class="form-control" id="unit" data-controlled="|rent|un-sell|" style="display:none;">
                  <option>Tháng</option>
                  <option>Năm</option>
                </select>                   
              </div>    
            </li>   
            <li class="form-group">                          
              <div class="col-sm-4 col-sm-offset-3 checkbox">
                <label><input type="checkbox" id="is_negotiable"> Có thể thương lượng</label>
              </div>                
            </li>            
          </ul>
        </li>
        <!-- #End item "Giá bán mong muốn" -->
        <!-- #Start item "Vị trí" -->
        <li class="item">
          <div class="item-title">Vị trí</div>
          <ul class="item-content">
            <li class="form-group">
              <label class="control-label col-sm-3" for="province_id">Tỉnh/Thành phố:</label>
              <div class="col-sm-4">
                <select name="province_id" class="form-control" id="province_id">
                  <option>TPHCM</option>
                  <option>Hà Nội</option>
                </select>
              </div>
            </li>
            <li class="form-group">            
              <label class="control-label col-sm-3" for="district_id">Quận/Huyện/Thị xã:</label>
              <div class="col-sm-4">
                <select name="district_id" class="form-control" id="district_id">
                  <option>Quận 1</option>
                  <option>Quận 2</option>
                  <option>Quận 3</option>
                  <option>Quận 4</option>
                  <option>Quận 5</option>
                </select>
            </li>   
            <li class="form-group">
              <label class="control-label col-sm-3" for="ward_id">Phường:</label>
              <div class="col-sm-4">
                <select name="ward_id" class="form-control" id="ward_id">
                  <option>Cầu Ông Lãnh</option>
                  <option>Đakao</option>
                  <option>Cầu Kho</option>
                </select>
              </div>
            </li> 
            <li class="form-group">            
              <label class="control-label col-sm-3" for="street_id">Đường:</label>
              <div class="col-sm-4">
                <select name="street_id" class="form-control" id="street_id">
                  <option>Trần Hưng Đạo</option>
                  <option>Điện Biên Phủ</option>
                  <option>Cô Giang</option>
                </select>
              </div>
            </li>
            <li class="form-group">            
              <label class="control-label col-sm-3" for="address_number">Số nhà:</label>
              <div class="col-sm-4">
                <input name="address_number" class="form-control" id="address_number">
              </div>
            </li>
            <li class="form-group">
              <label class="control-label col-sm-3" for="street_type">Loại Đường:</label>
              <div class="col-sm-4">
                <select name="street_type" class="form-control" id="street_type">
                  <option>Đường chính</option>
                  <option>Đường nội bộ</option>
                </select>
              </div>
            </li>
            <li class="form-group">              
              <div class="col-xs-6 col-sm-2 col-sm-offset-3 radio">
                <label><input type="radio" name="is_alley" class="will-change-show-check" value="Mặt tiền" data-hide="alley" checked=""> Mặt tiền</label>
              </div>
              <div class="col-xs-6 col-sm-2 radio">  
                <label><input type="radio" name="is_alley" class="will-change-show-check" value="Hẻm" data-show="alley"> Hẻm</label>
              </div>
              <div class="col-xs-4 col-xs-offset-7 col-sm-2 input-group" data-controlled="|alley|" hidden>
                <input type="text" class="form-control">
                <div class="input-group-addon">m</div>
              </div>                  
            </li>
            <li class="form-group">              
              <div class="col-sm-4 col-sm-offset-3">
                <a href="#" class="expand btn btn-link" data-target="#map">Xem vị trí trên bản đồ</a>
              </div>
              <div class="col-sm-4">
                <a href="#" class="btn btn-link" id="set_price_area">Định giá khu vực</a>                    
              </div>
            </li>
            <li class="form-group">            
              <div class="col-xs-12">
                <img src="/assets/product/icon_Bando.png" class="img-responsive" id="map" style="display:none;">
              </div>
            </li>              
          </ul>
        </li>
        <!-- #End item "Vị trí" -->
        <!-- #Start item "Loại hình bất động sản" -->
        <li class="item">
          <div class="item-title">
            Loại hình bất động sản
            <a href="#" class="support-button" data-value="#support_property_type"></a>
            <section class="support-text" id="support_property_type">                   
              <b>Diện tích xây dựng:</b> Là phần đất có công trình xây dựng.<br>
              <b>Diện tích sử dụng:</b> Là tổng diện tích các sàn xây dựng kể cả ban công, tầng hầm, sân thượng.<br>
              <b>Diện tích khuôn viên:</b> Là tổng diện tích đất bao gồm cả phần diện tích vi phạm lộ giới, quy hoạch không được công nhận.
            </section>
          </div>
          <ul class="item-content">
            <li class="form-group">            
              <label class="control-label col-sm-3" for="property_type">Loại hình:</label>
              <div class="col-sm-4" id="groupLoaiHinh">
                <select class="form-control will-change-select" id="property_type">
                  <option data-show="land" data-hide="un-land">Đất</option>
                  <option data-show="house" data-hide="un-house">Nhà</option>
                  <option data-show="apartment" data-hide="un-apartment">Căn hộ</option>
                  <option data-show="space" data-hide="un-space">Mặt bằng</option>
                </select>
              </div>
              <label class="control-label col-sm-1" for="property_type_details">&gt;</label>              
              <div class="col-sm-4">
                <select class="form-control will-change-select" id="property_type_details" data-controlled="|land|un-house|un-apartment|un-space|">
                  <option>Đất thổ cư</option>
                  <option>Trồng cây</option>
                  <option>Khác</option>
                </select>
                <select class="form-control will-change-select" data-controlled="|un-land|house|un-apartment|un-space|" style="display:none;">
                  <option data-show="home-city" data-hide="un-home-city">Nhà phố</option>
                  <option data-show="villa" data-hide="un-villa">Biệt thự</option>                      
                </select>
                <select class="form-control will-change-select" data-controlled="|un-land|un-house|apartment|un-space|" style="display:none;">
                  <option data-show="apartment-building" data-hide="un-apartment-building">Chung cư</option>
                  <option data-show="luxury-apartment" data-hide="un-luxury-apartment">Căn hộ cao cấp</option>                      
                </select>
                <select class="form-control" data-controlled="|un-land|un-house|un-apartment|space|" style="display:none;">
                  <option>Văn phòng</option>
                  <option>Phòng trọ</option>
                  <option>Mặt bằng-Cửa hàng</option>                      
                  <option>Nhà hàng-khách sạn</option>
                  <option>Nhà kho-Xưởng</option>
                </select>
              </div>
            </li>
            <li class="form-group sub-item" data-controlled="|un-land|house|un-apartment|space|" hidden>
              <label class="control-label col-sm-4" for="construction_area">Diện tích xây dựng:</label>
              <div class="col-sm-3 input-group">
                <input class="form-control only-number required" name="construction_area" id="construction_area">
                <div class="input-group-addon">m<sup>2</sup></div>
              </div>              
            </li>
            <li class="form-group sub-item" data-controlled="|un-land|house|apartment|space|" hidden>            
              <label class="control-label col-sm-4" for="using_area">Diện tích sử dụng:</label>
              <div class="col-sm-3 input-group">
                <input class="form-control only-number required" name="using_area" id="using_area">
                <div class="input-group-addon">m<sup>2</sup></div>
              </div>              
            </li>
            <li class="form-group sub-item" data-controlled="|land|house|un-apartment|space|">            
              <label class="control-label col-sm-4" for="campus_area">Diện tích khuôn viên:</label>
              <div class="col-sm-3 input-group">
                <input class="form-control only-number required" name="campus_area" id="campus_area">
                <div class="input-group-addon">m<sup>2</sup></div>
              </div>              
            </li>
            <li class="form-group sub-item" data-controlled="|land|house|apartment|space|">            
              <label class="control-label col-sm-4" for="width_x">Ngang:</label>
              <div class="col-sm-3 input-group">
                <input class="form-control only-number required" name="width_x" id="width_x">
                <div class="input-group-addon">m</div>
              </div>              
            </li>
            <li class="form-group sub-item" data-controlled="|land|house|apartment|space|">            
              <label class="control-label col-sm-4" for="width_y">Dài:</label>
              <div class="col-sm-3 input-group">
                <input class="form-control only-number required" name="width_y" id="width_y">
                <div class="input-group-addon">m</div>
              </div>              
            </li>
            <li class="form-group sub-item" data-controlled="|land|house|apartment|space|">
              <label class="control-label col-sm-4" for="shape">Hình dáng:</label>          
              <div class="col-sm-4">
                <select class="form-control will-change-select">
                  <option data-hide="normal-shape">Bình thường</option>
                  <option data-show="normal-shape">Nở hậu</option>
                  <option data-show="normal-shape">Tóp hậu</option>                       
                </select>
              </div>
              <div class="col-sm-2 col-xs-5 col-xs-offset-4 input-group" data-controlled="|normal-shape|" hidden>
                <input name="Do_NoTopHau" class="form-control only-number required" size="2" id="shape_width">
                <div class="input-group-addon">m</div>
              </div>              
            </li>
            <li class="form-group sub-item" data-controlled="|sell|un-rent|">                          
              <div class="col-sm-6 col-sm-offset-4 col-xs-12" id="plan_table"><a href="#" class="btn btn-link">Bảng quy hoạch chi tiết</a></div>
            </li>
            <li class="form-group sub-item" data-controlled="|un-land|house|apartment|un-space|" hidden>            
              <label class="control-label col-sm-4" for="floor_number">Số tầng:</label>
              <div class="col-sm-3 col-xs-12">
                <select class="form-control" name="floor_number" id="floor_number">
                  <option>Trệt</option>
                  <option>1</option>
                  <option>2</option>
                  <option>3</option>
                  <option>&gt;4</option>
                </select>
              </div>
            </li>                          
            <li class="form-group sub-item" data-controlled="|un-land|house|apartment|space|" hidden>            
              <label class="control-label col-sm-4" for="restroom_number">WC:</label>
              <div class="col-sm-3 col-xs-12">
                <select class="form-control" name="restroom_number" id="restroom_number">                       
                  <option>1</option>
                  <option>2</option>
                  <option>3</option>
                  <option>&gt;4</option>
                </select>
              </div>                      
            </li>
            <li class="form-group sub-item" data-controlled="|un-land|house|apartment|space|" hidden>            
              <label class="control-label col-sm-4" for="bedroom_number">Phòng ngủ:</label>
              <div class="col-sm-3 col-xs-12">
                <select class="form-control" name="bedroom_number" id="bedroom_number">                       
                  <option>0</option>
                  <option>1</option>
                  <option>2</option>
                  <option>3</option>
                  <option>&gt;4</option>
                </select>
              </div>
            </li>
            <li class="form-group sub-item" data-controlled="|un-land|house|apartment|space|" hidden>            
              <label class="control-label col-sm-4" for="direction">Hướng nhà:</label>
              <div class="col-sm-3 col-xs-12">
                <select class="form-control" name="direction" id="direction">     
                  <option>Đông</option>
                  <option>Tây</option>
                  <option>Nam</option>
                  <option>Bắc</option>
                  <option>Đông-Bắc</option>
                  <option>Đông-Nam</option>
                  <option>Tây-Bắc</option>
                  <option>Tây-Nam</option>
                </select>
              </div>                       
            </li>
            <li class="form-group sub-item" data-controlled="|un-land|house|apartment|space|" hidden>            
              <label class="control-label col-sm-4" for="build_year">Năm xây dựng:</label>
              <div class="col-sm-3">
                <select class="form-control" name="build_year" id="build_year">
                  <!-- js Xử lý-->                        
                </select>
              </div>                    
            </li>            
            <li class="form-group sub-item" data-controlled="|un-land|house|apartment|space|un-villa|home-city|un-luxury-apartment|apartment-building|" hidden>            
              <label class="control-label col-sm-4" for="construction_level">Phân loại nhà:</label>
              <div class="col-sm-3">
                <select class="form-control" name="construction_level" id="construction_level">     
                  <option>Nhà cấp 1</option>
                  <option>Nhà cấp 2</option>
                  <option>Nhà cấp 3</option>
                  <option>Nhà cấp 4</option>
                  <option>Nhà tạm</option>                        
                </select>
              </div>                  
            </li>
            <li class="form-group sub-item" data-controlled="|un-land|house|apartment|space|" hidden>            
              <label class="control-label col-sm-4" for="construction_quality">Chất lượng nhà còn lại:</label>
              <div class="col-sm-3 input-group">
                <input class="form-control only-number required" name="construction_quality" id="construction_quality">
                <div class="input-group-addon">%</div>
              </div>                  
            </li>            
          </ul>
        </li>
        <!-- #End item "Loại hình bất động sản" -->
        <!-- #Start item "Nội dung tin đăng" -->
        <li class="item">
          <div class="item-title">Nội dung tin đăng</div>
          <ul class="item-content">            
            <li class="form-group">            
              <label class="control-label col-sm-3" for="title" style="width=100%">Tiêu đề:</label>
              <div class="col-sm-9">
                <input name="title" class="form-control required" id="title">
              </div>
            </li>
            <li class="form-group">            
              <label class="control-label col-sm-3" for="description">Nội dung:</label>
              <div class="col-sm-9">
                <textarea name="description" class="form-control required" id="description"></textarea>                   
              </div>
            </li>            
          </ul>
        </li>
        <!-- #End item "Nội dung tin đăng" -->
        <!-- #Start item "Hồ sơ pháp lý" -->
        <li class="item continue-composing" data-controlled="|sell|un-rent|" hidden>
          <div class="item-title">Hồ sơ pháp lý</div>
          <ul class="item-content">
            <li class="form-group">            
              <label class="control-label col-sm-3" for="legal_records">Loại:</label>
              <div class="col-sm-4">
                <select name="legal_records" class="form-control will-change-select" id="legal_records">\
                  <option data-hide="different-legal">Sổ đỏ</option>
                  <option data-hide="different-legal">Sổ hồng</option>
                  <option data-show="different-legal">Khác</option>
                </select>
              </div>               
            </li>
            <li class="form-group" data-controlled="|different-legal|" hidden>            
              <label class="control-label col-sm-3 hidden-xs"></label>
              <div class="col-sm-4">
                <input name="legal_record_different" class="form-control required" id="legal_record_different">
              </div>                      
            </li>            
          </ul>
        </li>
        <!-- #End item "Hồ sơ pháp lý" -->
        <!-- #Start item "Tình trạng quy hoạch" -->
        <li class="item continue-composing" data-controlled="|sell|un-rent|" hidden>
          <div class="item-title">Tình trạng quy hoạch</div>
          <ul class="item-content">
            <li class="form-group">            
              <label class="control-label col-sm-3" for="planning_status">Loại:</label>
              <div class="col-sm-4">
                <select name="planning_status" class="form-control will-change-select" id="planning_status">\
                  <option data-hide="different-planning">Ổn định</option>
                  <option data-hide="different-planning">Một phần</option>
                  <option data-hide="different-planning">Toàn bộ</option>
                  <option data-show="different-planning">Khác</option>
                </select>
              </div>
            </li>
            <li class="form-group" data-controlled="|different-planning|" hidden>            
              <label class="control-label col-sm-3 hidden-xs"></label>
              <div class="col-sm-4">
                <input name="planning_status_different" class="form-control required" id="planning_status_different">
              </div>               
            </li>              
          </ul>
        </li>
        <!-- #End item "Tình trạng quy hoạch" -->
        <!-- #Start item "Tiện nghi khác" -->
        <li class="item continue-composing" hidden>
          <div class="item-title">Tiện nghi khác</div>
          <ul class="item-content">
            <li class="form-group">            
              <div class="checkbox">                    
                <label class="col-xs-6 col-sm-3 col-md-4 col-lg-3"><input type="checkbox"> Tầng hầm</label>
                <label class="col-xs-6 col-sm-3 col-md-4 col-lg-3"><input type="checkbox"> Tầng lửng</label>
                <label class="col-xs-6 col-sm-3 col-md-4 col-lg-3"><input type="checkbox"> Sân thượng</label>
                <label class="col-xs-6 col-sm-3 col-md-4 col-lg-3"><input type="checkbox"> Gara ôtô</label>           
                <label class="col-xs-6 col-sm-3 col-md-4 col-lg-3"><input type="checkbox"> Thang máy</label>
                <label class="col-xs-6 col-sm-3 col-md-4 col-lg-3"><input type="checkbox"> Hồ bơi</label>                   
              </div>
            </li>            
          </ul>
        </li>
        <!-- #End item "Tiện nghi khác" -->
        <!-- #Start item "Tiện ích khu vực" -->
        <li class="item continue-composing" hidden>
          <div class="item-title">Tiện ích khu vực</div>
          <ul class="item-content">
            <li class="form-group">            
              <div class="checkbox">
                <label class="col-xs-6 col-sm-3 col-md-4 col-lg-3"><input type="checkbox"> Siêu thị</label>
                <label class="col-xs-6 col-sm-3 col-md-4 col-lg-3"><input type="checkbox"> Chợ</label>
                <label class="col-xs-6 col-sm-3 col-md-4 col-lg-3"><input type="checkbox"> Bệnh viện</label>
                <label class="col-xs-6 col-sm-3 col-md-4 col-lg-3"><input type="checkbox"> Công viên</label>      
                <label class="col-xs-6 col-sm-3 col-md-4 col-lg-3"><input type="checkbox"> Khu vui chơi</label>
                <label class="col-xs-6 col-sm-3 col-md-4 col-lg-3"><input type="checkbox"> Bến xe</label>
                <label class="col-xs-6 col-sm-3 col-md-4 col-lg-3"><input type="checkbox"> Khu dân cư</label>
                <label class="col-xs-6 col-sm-3 col-md-4 col-lg-3"><input type="checkbox"> Khu kinh doanh</label>         
              </div>
            </li>           
