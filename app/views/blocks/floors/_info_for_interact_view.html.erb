<%
=begin
	params: @floor
=end
%>

Danh sách sản phẩm
<ul>
	<% @floor.real_estates.each do |re| %>
		<li>
			<a aria-click="interact" data-type="real_estate" data-value="<%= re.id %>" data-data="<%= { from: 'surface' }.to_json %>"><%= re.label %></a>
		</li>
	<% end %>
</ul>

Danh sách mặt cắt khác
<ul>
	<% @floor.block.floors.each do |floor| %>
		<% next if floor.id == @floor.id %>
		<li>
			<a aria-click="interact" data-type="blocks/floor" data-value="<%= floor.id %>"><%= floor.name %></a>
		</li>
	<% end %>
</ul>