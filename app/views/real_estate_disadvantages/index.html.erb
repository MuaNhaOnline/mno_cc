<p id="notice"><%= notice %></p>

<h1>Listing Real Estate Disadvantages</h1>

<table>
  <thead>
    <tr>
      <th>Real estate</th>
      <th>Disadvantage</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @real_estate_disadvantages.each do |real_estate_disadvantage| %>
      <tr>
        <td><%= real_estate_disadvantage.real_estate_id %></td>
        <td><%= real_estate_disadvantage.disadvantage_id %></td>
        <td><%= link_to 'Show', real_estate_disadvantage %></td>
        <td><%= link_to 'Edit', edit_real_estate_disadvantage_path(real_estate_disadvantage) %></td>
        <td><%= link_to 'Destroy', real_estate_disadvantage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Real estate disadvantage', new_real_estate_disadvantage_path %>
