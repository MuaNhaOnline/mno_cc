<p id="notice"><%= notice %></p>

<h1>Listing Real Estate Advantages</h1>

<table>
  <thead>
    <tr>
      <th>Real estate</th>
      <th>Advantage</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @real_estate_advantages.each do |real_estate_advantage| %>
      <tr>
        <td><%= real_estate_advantage.real_estate_id %></td>
        <td><%= real_estate_advantage.advantage_id %></td>
        <td><%= link_to 'Show', real_estate_advantage %></td>
        <td><%= link_to 'Edit', edit_real_estate_advantage_path(real_estate_advantage) %></td>
        <td><%= link_to 'Destroy', real_estate_advantage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Real estate advantage', new_real_estate_advantage_path %>
